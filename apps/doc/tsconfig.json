{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "references": [
    {
      "path": "../../packages/ui-react"
    },
    {
      "path": "../../packages/tailwind"
    },
    {
      "path": "../../packages/theme"
    }
  ]
}